<%-include("partials/Header",{title:"postform"})%>
<%-include("partials/Navigation" )%>
<form method="POST" action="/mypost" class="container mx-auto col-4" enctype="multipart/form-data" > 
  
  <h1>This is a post form </h1>
  <div class="mb-3">
    <label for="firstname" class="form-label">firstame</label>
    <input type="text" class="form-control" name="firstame" id="firstname" aria-describedby="firstname"> 
  </div>
  <div class="mb-3">
    <label for="lastname" class="form-label">lastname</label>
    <input type="text" class="form-control" name="lastname" id="lastname">
  </div> 
  <div class="mb-3">
    <label for="samplefile" class="form-label">samplefile</label>
    <input type="file" class="form-control" name="samplefile" id="lastname" multiple>
  </div> 
  <button type="submit" class="btn btn-primary">Submit</button>
</form>
<%-include("partials/Footer",{title:"getForm"})%>